<!--Include header file-->
<%- include('include/_header')%>
<form autocomplete="off" class="appForm clearfix" method="post" enctype="application/x-www-form-urlencoded">
    <fieldset>
        <legend>Create new account</legend>
        <div class="input_wrapper n90 border">
            <label class="floated">Name</label>
            <input required type="text" name="userName" id="userName" maxlength="15" value="">
        </div>
        <div class="input_wrapper n90 border">
            <label class="floated">Password</label>
            <input required type="password" name="password" id="" maxlength="60" value="">
        </div>
        <div class="input_wrapper n90 border">
            <label class="floated">Conform Password</label>
            <input required type="password" name="cPassword" id="" maxlength="60" value="">
        </div>
        <div class="input_wrapper n90 border">
            <label class="floated">Email</label>
            <input required type="email" name="email" id="" maxlength="50" value="">
        </div>
        <div class="input_wrapper n90 border">
            <label class="floated">Confirm Email</label>
            <input required type="email" name="cEmail" id="Privilege" maxlength="50" value="">
        </div>
        <div class="input_wrapper n90 border">
            <label class="floated">Role</label>
            <input required type="number" name="phone" id="" maxlength="15" value="">
        </div>
        <input style="text-align:center;float:center;" class="no_float" type="submit" name="submit" value="Save">
    </fieldset>
</form>
<!--Include footer file-->
<%- include('include/_footer')%>
